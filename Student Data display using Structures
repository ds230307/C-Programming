#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
struct student{
    char name[50];
    int age;
    char grade;
};
int main() {
    struct student s1;
    scanf("%[^\n]%*c", s1.name);
    scanf("%d ", &s1.age);
    scanf("%[^\n]%*c", &s1.grade);
    printf("Name: %s\n", s1.name);
    printf("Age: %d\n", s1.age);
    printf("Grade: %c\n", s1.grade);

      
    return 0;
}
