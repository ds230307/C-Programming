#include<stdio.h>
int main(){
    int rowsize, colsize;
    printf("Enter row and column size :");
    scanf("%d%d", &rowsize, &colsize);
    int marks[rowsize][colsize];
    printf("Enter 2D Array Elemnts :");
    for(int i=0; i<rowsize; i++){
        for(int j=0; j<colsize; j++){
            scanf("%d", &marks[i][j]);
        }
    }
    printf("\n\n");
    for(int i=0; i<rowsize; i++){
        for(int j=0; j<colsize; j++){
            printf("%d", marks[i][j]);
        }
        printf("\n");
    }
    return 0;
    }
