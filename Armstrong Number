#include <stdio.h>
#include <math.h>

int main() {
    int N, original, rem, result = 0, n = 0;
    scanf("%d", &N);
    original = N;
    int temp = N;
    while (temp != 0) {
        temp /= 10;
        n++;
    }
    temp = N;
    while (temp != 0) {
        rem = temp % 10;
        result += pow(rem, n);
        temp /= 10;
    }
    if (result == original)
        printf("Armstrong\n");
    else
        printf("Not Armstrong\n");

    return 0;
}
