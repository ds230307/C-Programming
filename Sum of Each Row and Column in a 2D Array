#include <stdio.h>
int main() {
    int R, C;
    scanf("%d %d", &R, &C);
    int matrix[R][C];
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            scanf("%d", &matrix[i][j]);
        }
    }
    for (int i = 0; i < R; i++) {
        int rowSum = 0;
        for (int j = 0; j < C; j++) {
            rowSum += matrix[i][j];
        }
        printf("%d\n", rowSum);
    }
    for (int j = 0; j < C; j++) {
        int colSum = 0;
        for (int i = 0; i < R; i++) {
            colSum += matrix[i][j];
        }
        printf("%d\n", colSum);
    }
    return 0;
}
