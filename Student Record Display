#include <stdio.h>

int main() {
    int roll;
    char name[100], branch[100], section[10], college[200];

    scanf("%d", &roll);
    getchar(); 
    gets(name);
    gets(branch);
    gets(section);
    gets(college);

    printf("Roll Number: %d\n", roll);
    printf("Name: %s\n", name);
    printf("Branch: %s\n", branch);
    printf("Section: %s\n", section);
    printf("College: %s\n", college);

    return 0;
}
