#include <stdio.h>
int main() {
    int R, C;
    scanf("%d %d", &R, &C);
    int A[100][100], T[100][100];
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            scanf("%d", &A[i][j]);
        }
    }
    for (int i = 0; i < R; i++) {
        for (int j = 0; j < C; j++) {
            T[j][i] = A[i][j];
        }
    }
    for (int i = 0; i < C; i++) {
        for (int j = 0; j < R; j++) {
            printf("%d", T[i][j]);
            if (j < R - 1) printf(" ");
        }
        printf("\n");
    }
    return 0;
}
