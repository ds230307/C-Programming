#include <stdio.h>

int main() {
    int N,max;
    scanf("%d", &N);  
    int arr[100];        
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    max = arr[0];  
    for (int i = 1; i < N; i++) {
        if (arr[i] > max) {
            max = arr[i];
        }
    }
    printf("%d\n", max);  
    return 0;
}
