#include <stdio.h>
int main()
{
    int i, n, a = 0, b= 1, c;
    printf("Enter the number of terms:");
    scanf("%d", &n);
    if(n==1)
    printf("%d", a);
    else{
        printf("%d %d", a, b);
        for(i=3; i<=n; i++){
            c = a + b;
            printf(", %d", c);
            a = b;
            b = c;
        }
    
    }
    return 0;
}
