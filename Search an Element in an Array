#include <stdio.h>

int main() {
    int N, key;
    scanf("%d", &N);
    int arr[100]; 
    for (int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    scanf("%d", &key);
    int found = 0;
    for (int i = 0; i < N; i++) {
        if (arr[i] == key) {
            printf("Element found at position %d\n", i + 1); 
            found = 1;
            break;
        }
    }
    if (!found) {
        printf("Element not found\n");
    }
    return 0;
}
