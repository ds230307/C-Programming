#include<stdio.h>
#include<ctype.h>
int main(){
    char ch;
    printf("Enter Character:");
    scanf("%c", &ch);
    if(isalpha(ch)){
   if(islower(ch));
   ch = toupper(ch);
   if (ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U')
   printf("Vowel");
   else
   printf("Not Vowel");
    }
    else
    printf("Invalid Input!!!");
   return 0;
}
