#include <stdio.h>
#include <ctype.h> 

int main() {
    char str[1001];
    int alphabets = 0, digits = 0, special = 0;
    int i = 0;
    scanf("%[^\n]", str);
    while (str[i] != '\0') {
        if (isalpha(str[i])) {
            alphabets++;
        } else if (isdigit(str[i])) {
            digits++;
        } else if (str[i] != ' ') { 
            special++;
        }
        i++;
    }

    printf("%d %d %d", alphabets, digits, special);
    return 0;
}
