#include <stdio.h>
int main() {
    int n;
    scanf("%d", &n);
    int A[n], B[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &A[i]);
    }
    for (int i = 0; i < n; i++) {
        scanf("%d", &B[i]);
    }
    int *pA = A;
    int *pB = B;
    for (int i = 0; i < n; i++) {
        int temp = *(pA + i);
        *(pA + i) = *(pB + i);
        *(pB + i) = temp;
    }
    for (int i = 0; i < n; i++) {
        printf("%d ", *(pA + i));
    }
    printf("\n");
    for (int i = 0; i < n; i++) {
        printf("%d ", *(pB + i));
    }
    return 0;
}
