// WAP to extract all the digits of a given number and display them
#include<stdio.h>
int main(){
    char ch;
    do{
    int x,n, num;
    printf("Enter the number=", n);
    scanf("%d", &n);
    while(n>0){
        x=n%10;
        printf("%d\t", x);
        n=n/10;

    }
    printf("\nDo you want to continue again press (Y/N)");
    fflush(stdin);
    scanf("%c", &ch);
    }while(ch== 'Y' || ch=='y');
    return 0;
}
