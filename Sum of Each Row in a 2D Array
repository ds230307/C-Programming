#include <stdio.h>
int main() {
    int R, C;
    scanf("%d %d", &R, &C);
    for (int i = 0; i < R; i++) {
        int rowSum = 0;
        for (int j = 0; j < C; j++) {
            int val;
            scanf("%d", &val);
            rowSum += val;
        }
        printf("%d\n", rowSum);
    }
    return 0;
}
