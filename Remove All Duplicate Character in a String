#include <stdio.h>
#include <string.h>

int main() {
    char str[1001], result[1001];
    int i, j, k = 0;
    int found;
    fgets(str, sizeof(str), stdin);
    size_t len = strlen(str);
    if (len > 0 && str[len - 1] == '\n') {
        str[len - 1] = '\0';
    }
    for (i = 0; str[i] != '\0'; i++) {
        found = 0;
        for (j = 0; j < k; j++) {
            if (str[i] == result[j]) {
                found = 1;
                break;
            }
        }
        if (!found) {
            result[k++] = str[i];
        }
    }
    result[k] = '\0';  
    printf("%s\n", result);
    return 0;
}
