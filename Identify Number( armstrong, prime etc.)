#include <stdio.h>
#include <math.h>
int isArmstrong(int num) {
    int original = num, sum = 0, digits = 0, temp = num;
    while (temp != 0) {
        digits++;
        temp /= 10;
    }
    temp = num;
    while (temp != 0) {
        int digit = temp % 10;
        sum += pow(digit, digits);
        temp /= 10;
    }
    return sum == original;
}
int isPrime(int num) {
    if (num <= 1) return 0;
    for (int i = 2; i <= sqrt(num); i++) {
        if (num % i == 0)
            return 0;
    }
    return 1;
}
int isPerfect(int num) {
    int sum = 0;
    for (int i = 1; i <= num / 2; i++) {
        if (num % i == 0)
            sum += i;
    }
    return sum == num;
}
int main() {
    int num;
    scanf("%d", &num);

    if (isArmstrong(num))
        printf("Armstrong Number\n");
    else if (isPrime(num))
        printf("Prime Number\n");
    else if (isPerfect(num))
        printf("Perfect Number\n");
    else
        printf("None of these\n");

    return 0;
}
