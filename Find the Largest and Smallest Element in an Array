#include <stdio.h>
int main() {
    int N;
    scanf("%d", &N);
    int arr[100];
    for(int i = 0; i < N; i++) {
        scanf("%d", &arr[i]);
    }
    int smallest = arr[0];
    int largest = arr[0];
    for(int i = 1; i < N; i++) {
        if(arr[i] < smallest)
            smallest = arr[i];
        if(arr[i] > largest)
            largest = arr[i];
    }
    printf("%d %d", smallest, largest);
    return 0;
}
